    "@remkoj/optimizely-graph-client": "file:./packages/dist/@remkoj-optimizely-graph-client-dev.tgz",
    "@remkoj/optimizely-one-nextjs": "file:./packages/dist/@remkoj-optimizely-one-nextjs-dev.tgz"
  }
